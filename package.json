{
  "name": "social-icon-buttons",
  "version": "1.4.0",
  "description": "WordPress plugin for embedding simple social share buttons with font awesome icons",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:css": "parcel watch inc/social-icons.css -d inc -o social-icons.min.css --no-hmr --public-url .",
    "build:css": "parcel build inc/social-icons.css -d inc -o social-icons.min.css --public-url .",
    "dev:js": "parcel watch inc/social-icons.js -d inc -o social-icons.min.js --no-hmr --public-url . --experimental-scope-hoisting",
    "build:js": "parcel build inc/social-icons.js -d inc -o social-icons.min.js --public-url . --experimental-scope-hoisting",
    "build:html": "parcel build inc/icons.html -d inc -o icons.min.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/16patsle/social-icon-buttons.git"
  },
  "author": "Patrick Sletvold <patricksletvold@hotmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/16patsle/social-icon-buttons/issues"
  },
  "homepage": "https://github.com/16patsle/social-icon-buttons#readme",
  "devDependencies": {
    "parcel-bundler": "^1.12.4"
  },
  "browserslist": [
    "> 0.25%",
    "not ie <= 11",
    "not op_mini all",
    "not dead"
  ]
}
